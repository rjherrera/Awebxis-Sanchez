
<div class="form-container">
  <form action="<%= contactPostPath %>" method="post" enctype="multipart/form-data" class="form-card">
    <div>
      <label for="title">Title</label>
      <input type="text" name="title" placeholder="Charlie and the Chocolate Factory" required />
    </div>
    <div>
      <label for="author">Author</label>
      <input type="text" name="author" placeholder="Roald Dahl" required />
    </div>
    <div class="button">
      <input type="submit" name="create" value="Send" />
    </div>
  </form>
</div>
