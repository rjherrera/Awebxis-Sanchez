<h1>Genres</h1>

<a href="<%= newGenrePath %>">New Genre</a>

<ul>
  <% genres.forEach((genre) => { %>
    <li><a href="<%= buildGenrePath(genre) %>"><%= genre.name %></a></li>
  <% }) %>
</ul>

<% if (page > 1) { %>
<a href="<%= previousPagePath %>">Previous</a>
<% } %>
<p>Page <%= page %></p>
<a href="<%= nextPagePath %>">Next</a>
